<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<window xmlns="http://schemas.haulmont.com/cuba/screen/window.xsd"
        caption="msg://dataSetViewEdit.caption"
        focusComponent="form"
        messagesPack="ch.so.agi.simi.web.screens.product">
    <data>
        <instance id="dataSetViewDc"
                  class="ch.so.agi.simi.entity.product.DataSetView">
            <view extends="_local"/>
            <loader/>
        </instance>
    </data>
    <dialogMode height="AUTO"
                width="AUTO"
                modal="true"
                forceDialog="true"/>
    <layout expand="editActions" spacing="true">
        <form id="form" dataContainer="dataSetViewDc">
            <column width="350px">
                <textField id="identifierField" property="identifier"/>
            </column>
        </form>
        <hbox id="editActions" spacing="true">
            <button action="windowCommitAndClose"/>
            <button action="windowClose"/>
        </hbox>
    </layout>
</window>
